{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "SqlConnection": "Host=${POSTGRES_HOST};Port=${POSTGRES_PORT};Username=${POSTGRES_USER};Password=${POSTGRES_PASSWORD};Database=${POSTGRES_DB};Pooling=true;",
    "AzureBlobStorage": "${BLOB_STORAGE_CONNECTION_STRING}"
  },

  "Jwt": {
    "Key": "${JWT_KEY}",
    "LifeTimeInMinutes": "${JWT_LIFE_TIME_IN_MINUTES}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  },

  "OAuth": {
    "GoogleOAuth": {
      "ClientId": "${GOOGLE_OAUTH_CLIENT_ID}",
      "ClientSecret": "${GOOGLE_OAUTH_CLIENT_SECRET}",
      "CallbackPath": "${GOOGLE_OAUTH_CALLBACK_PATH}"
    }
  },

  "RabbitMq": {
    "HostName": "${RABBIT_MQ_HOSTNAME}",
    "UserName": "${RABBIT_MQ_USERNAME}",
    "Password": "${RABBIT_MQ_PASSWORD}"
  },

  "SmtpOptions": {
    "Port": "${EMAIL_PORT}",
    "Server": "${EMAIL_SERVER}",
    "User": "${EMAIL_USER}",
    "Email": "${EMAIL_EMAIL}",
    "Name": "${EMAIL_NAME}",
    "Password": "${EMAIL_PASSWORD}"
  },

  "Redirect": {
    "Scheme": "${REDIRECT_SCHEME}",
    "Host": "${REDIRECT_HOST}",
    "Path": "${REDIRECT_PATH}"
  },

  "BlobStorage": {
    "ContainerName": "${BLOB_STORAGE_CONTAINER_NAME}",
    "Directory": "${BLOB_STORAGE_DIRECTORY}"
  }
}

